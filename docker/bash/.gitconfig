[user]
    name = NAME
    email = EMAIL
[alias]
	fe = fetch
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	lola = log --graph --decorate --oneline
[push]
	default = simple
[core]
	autocrlf = input
	editor = vim
[color]
	ui = true
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[credential]
	helper = store